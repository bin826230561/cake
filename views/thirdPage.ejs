<script>
  $("#surePay").click(function () {
    var allPrice = $("#price").text();
    var goodsname = $(':checked').parent().parent().prev().prev().children().eq(0).text()
    var goodsname2 = $(':checked').parent().parent().prev().prev().children().eq(1).text()

